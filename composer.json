{
  "name": "sinefineinfinitum/prom-import",
  "description": "Prom Ua Import",
  "minimum-stability": "stable",
  "type": "wordpress-plugin",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "sinefine"
    }
  ],
  "require": {
    "php": ">=8.0",
    "ext-simplexml": "*",
    "campo/random-user-agent": "*"
  },
  "require-dev": {
    "phpstan/extension-installer": "^1.4.3",
    "szepeviktor/phpstan-wordpress": "^2.0.3",
    "phpstan/phpstan": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "SineFine\\PromImport\\": "src/"
    }
  },
  "config": {
    "vendor-dir": "vendor",
    "allow-plugins": {
      "composer/installers": true,
      "phpstan/extension-installer": true
    }
  },
  "scripts": {
    "cs": [
      "phpstan analyse --memory-limit 1G"
    ]
  }
}
